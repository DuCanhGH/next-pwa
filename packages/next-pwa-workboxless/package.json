{
  "name": "@ducanh2912/next-pwa-workboxless",
  "version": "1.0.0",
  "type": "module",
  "description": "PWA for Next.js, powered by pure ServiceWorker.",
  "repository": "https://github.com/DuCanhGH/next-pwa",
  "author": "DuCanhGH",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "files": [
    "dist"
  ],
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/next-pwa-workboxless.cjs",
      "default": "./dist/next-pwa-workboxless.module.js"
    },
    "./workbox": {
      "types": "./dist/sw-entry.d.ts"
    }
  },
  "main": "./dist/next-pwa-workboxless.cjs",
  "module": "./dist/next-pwa-workboxless.module.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "workbox": [
        "./dist/sw-entry.d.ts"
      ]
    }
  },
  "keywords": [
    "next.js",
    "pwa",
    "web",
    "serviceworker"
  ],
  "scripts": {
    "dev": "rimraf dist && rollup -c -w",
    "build": "rimraf dist && cross-env NODE_ENV=production rollup -c --compact",
    "lint": "eslint src --cache --cache-location ./node_modules/.cache/eslint --ext ts,tsx,js,jsx,cjs,mjs",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@swc/core": "1.3.53",
    "clean-webpack-plugin": "4.0.0",
    "swc-loader": "0.2.3",
    "terser-webpack-plugin": "5.3.7"
  },
  "devDependencies": {
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.0.2",
    "@rollup/plugin-terser": "0.4.1",
    "@rollup/plugin-typescript": "11.1.0",
    "@types/node": "18.15.13",
    "chalk": "5.2.0",
    "next": "13.3.1",
    "rollup": "3.20.7",
    "tsconfig": "workspace:*",
    "typescript": "5.1.0-dev.20230422",
    "webpack": "5.80.0"
  },
  "peerDependencies": {
    "next": ">=9.1.0"
  }
}

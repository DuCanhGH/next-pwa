{
  "name": "next-pwa-workboxless",
  "version": "1.0.0",
  "type": "module",
  "description": "PWA for Next.js, powered by pure ServiceWorker.",
  "repository": "https://github.com/DuCanhGH/next-pwa",
  "author": "DuCanhGH",
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/next-pwa-workboxless.cjs",
      "default": "./dist/next-pwa-workboxless.modern.mjs"
    },
    "./workbox": {
      "types": "./dist/sw-entry.d.ts"
    }
  },
  "main": "./dist/next-pwa-workboxless.cjs",
  "module": "./dist/next-pwa-workboxless.module.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "workbox": [
        "./dist/sw-entry.d.ts"
      ]
    }
  },
  "keywords": [
    "next.js",
    "pwa",
    "web",
    "serviceworker"
  ],
  "scripts": {
    "dev": "rimraf dist && rollup -c -w",
    "build": "rimraf dist && cross-env NODE_ENV=production rollup -c --compact",
    "lint": "eslint src --cache --cache-location ./node_modules/.cache/eslint --ext ts,tsx,js,jsx,cjs,mjs",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-terser": "0.4.0",
    "@rollup/plugin-typescript": "11.0.0",
    "@swc/core": "1.3.32",
    "@types/node": "18.11.19",
    "clean-webpack-plugin": "4.0.0",
    "rollup": "3.13.0",
    "swc-loader": "0.2.3",
    "terser-webpack-plugin": "5.3.6",
    "tsconfig": "workspace:*",
    "typescript": "5.0.0-dev.20230204",
    "webpack": "5.75.0"
  },
  "peerDependencies": {
    "next": ">=9.1.0",
    "swc-loader": "0.x",
    "webpack": ">=5.9.0"
  },
  "peerDependenciesMeta": {
    "swc-loader": {
      "optional": true
    }
  }
}

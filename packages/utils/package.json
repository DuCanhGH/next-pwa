{
  "name": "@ducanh2912/utils",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "typesVersions": {
    "*": {
      "swc-config": [
        "./src/swc-config.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./swc-config": {
      "types": "./src/swc-config.d.ts",
      "default": "./src/swc-config.js"
    }
  },
  "scripts": {
    "dev": "rollup --config --watch",
    "build": "rimraf dist && cross-env NODE_ENV=production rollup --config --compact"
  },
  "dependencies": {
    "chalk": "5.3.0",
    "semver": "7.5.4"
  },
  "devDependencies": {
    "@ducanh2912/constants": "workspace:*",
    "@rollup/plugin-node-resolve": "15.2.1",
    "@rollup/plugin-swc": "0.2.0",
    "@swc/core": "1.3.78",
    "@types/semver": "7.5.1",
    "rollup": "3.29.1",
    "terser-webpack-plugin": "5.3.9",
    "type-fest": "4.3.1",
    "typescript": "5.3.0-dev.20230912"
  }
}

{
  "name": "@ducanh2912/next-sw",
  "version": "1.1.0",
  "type": "module",
  "description": "PWA for Next.js, powered by pure ServiceWorker.",
  "repository": "https://github.com/DuCanhGH/next-pwa",
  "author": "DuCanhGH",
  "license": "MIT",
  "files": [
    "dist",
    "!dist/dts"
  ],
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.module.d.ts",
        "default": "./dist/index.module.js"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "types": "./dist/index.module.d.ts",
  "keywords": [
    "next.js",
    "pwa",
    "web",
    "serviceworker"
  ],
  "scripts": {
    "dev": "rimraf dist && rollup --config --watch",
    "build": "rimraf dist && cross-env NODE_ENV=production rollup --config --compact",
    "lint": "eslint src --ext ts,tsx,js,jsx,cjs,mjs",
    "typecheck": "tsc"
  },
  "dependencies": {
    "clean-webpack-plugin": "4.0.0",
    "terser-webpack-plugin": "5.3.9"
  },
  "devDependencies": {
    "@ducanh2912/constants": "workspace:*",
    "@ducanh2912/utils": "workspace:*",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.2.1",
    "@rollup/plugin-swc": "0.2.0",
    "@rollup/plugin-typescript": "11.1.3",
    "@swc/core": "1.3.86",
    "@types/node": "20.6.3",
    "chalk": "5.3.0",
    "next": "13.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup": "3.28.1",
    "rollup-plugin-dts": "6.0.2",
    "typescript": "5.3.0-dev.20230921",
    "webpack": "5.88.2"
  },
  "peerDependencies": {
    "next": ">=9.1.0"
  }
}

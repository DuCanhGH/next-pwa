{
  "name": "@ducanh2912/next-pwa",
  "version": "5.6.0",
  "type": "module",
  "description": "PWA for Next.js, powered by Workbox.",
  "repository": "https://github.com/DuCanhGH/next-pwa",
  "author": "ShadowWalker <w@weiw.io>, DuCanhGH",
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/ducanh2912-next-pwa.js",
      "default": "./dist/ducanh2912-next-pwa.modern.mjs"
    }
  },
  "main": "./dist/ducanh2912-next-pwa.js",
  "module": "./dist/ducanh2912-next-pwa.module.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "nextjs",
    "pwa",
    "workbox",
    "web",
    "service-worker"
  ],
  "scripts": {
    "dev": "microbundle watch",
    "build": "rimraf dist && microbundle",
    "lint": "eslint src --cache --cache-location ./node_modules/.cache/eslint --ext ts,tsx,js,jsx,cjs,mjs",
    "typecheck": "tsc",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "babel-loader": "9.1.0",
    "clean-webpack-plugin": "4.0.0",
    "globby": "13.1.2",
    "terser-webpack-plugin": "5.3.6",
    "workbox-build": "6.5.4",
    "workbox-webpack-plugin": "6.5.4",
    "workbox-window": "6.5.4"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "husky": "8.0.2",
    "microbundle": "0.15.1",
    "next": "13.0.5",
    "prettier": "2.8.0",
    "prettier-plugin-jsdoc": "0.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "3.0.2",
    "typescript": "4.9.3",
    "webpack": "5.75.0"
  },
  "peerDependencies": {
    "next": ">=9.0.0"
  }
}

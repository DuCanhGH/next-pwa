{
  "name": "@ducanh2912/next-pwa-project",
  "type": "module",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "docs",
    "packages/**",
    "examples/**",
    "!examples/**/.next"
  ],
  "scripts": {
    "build": "cross-env FORCE_COLOR=1 turbo run build --filter=./packages/*",
    "deps": "node ncu.js",
    "dev": "cross-env FORCE_COLOR=1 turbo run dev --parallel --filter=./packages/*",
    "docs:build": "cross-env FORCE_COLOR=1 turbo run build --filter=./docs",
    "docs:lint": "cross-env FORCE_COLOR=1 turbo run lint --parallel --filter=./docs",
    "empty-chset": "changeset --empty",
    "examples:build": "cross-env FORCE_COLOR=1 turbo run build --filter=./examples/*",
    "examples:dev": "cross-env FORCE_COLOR=1 turbo run dev --parallel --filter=./examples/*",
    "examples:lint": "cross-env FORCE_COLOR=1 turbo run lint --parallel --filter=./examples/*",
    "format": "prettier --write",
    "format:all": "pnpm format .",
    "format:check": "prettier --check .",
    "lint": "cross-env FORCE_COLOR=1 turbo run lint --parallel --filter=./packages/*",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "publish-packages": "pnpm typecheck && pnpm lint && pnpm build && changeset publish",
    "test": "cross-env FORCE_COLOR=1 turbo run test --filter=./packages/*",
    "typecheck": "cross-env FORCE_COLOR=1 turbo run typecheck"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.2",
    "@ducanh2912/next-pwa": "workspace:*",
    "@ducanh2912/next-sw": "workspace:*",
    "@types/node": "20.4.2",
    "@types/shell-quote": "1.7.1",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "cross-env": "7.0.3",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.10",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-turbo": "1.10.8",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "fast-glob": "3.3.0",
    "husky": "8.0.3",
    "jest": "29.6.1",
    "lint-staged": "13.2.3",
    "npm-check-updates": "16.10.16",
    "prettier": "3.0.0",
    "prettier-plugin-tailwindcss": "0.4.1",
    "rimraf": "5.0.1",
    "shell-quote": "1.8.1",
    "tslib": "2.6.0",
    "turbo": "1.10.8",
    "typescript": "5.2.0-dev.20230718"
  },
  "packageManager": "pnpm@8.6.2"
}
